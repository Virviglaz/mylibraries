# Include this file in your Makefile for embedded project prio to Makefile.embedded

ASMFLAGS			+=	-mthumb \
					-g3 \
					-c

CFLAGS 				+=	-mthumb \
					-fdata-sections \
					-ffunction-sections \
					-g

LDFLAGS				+=	-mthumb \
					-Wl,--gc-sections \
					--specs=nosys.specs \
					-fno-exceptions
